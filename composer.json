{
	"name": "baraja-core/markdown-latte-filter",
	"description": "Tools for content rendering.",
	"homepage": "https://github.com/baraja-core/markdown-latte-filter",
	"authors": [
		{
			"name": "Jan Barášek",
			"homepage": "https://baraja.cz"
		}
	],
	"require": {
		"php": ">=7.4.0",
		"latte/latte": "^2.9",
		"nette/utils": "^3.2",
		"nette/application": "^3.1",
		"nette/di": "^3.0",
		"league/commonmark": "^0.18.3",
		"webuni/commonmark-table-extension": "^0.9.0"
	},
	"require-dev": {
		"phpstan/phpstan": "^0.12.18",
		"tracy/tracy": "^2.8",
		"phpstan/phpstan-nette": "^0.12.6",
		"symplify/easy-coding-standard": "^7.2"
	},
	"autoload": {
		"classmap": [
			"src/"
		]
	},
	"scripts": {
		"phpstan": [
			"vendor/bin/phpstan analyse src -c phpstan.neon --level 8 --no-progress"
		]
	},
	"minimum-stability": "stable"
}
